<h1>Book List</h1>
{{!-- table>tr*>th* or table>tr*>td* or --}}
<table style="width:100%; height:200px" class="mb-3">
    <tr>
        <th>Title</th>
        <th>Author</th>
        <th>Price</th>
        <th>Quantity</th>
        <th>Date</th>
    </tr>
        {{#each books}}
    <tr>
        <th>{{title}}</th>
        <th>{{author}}</th>
        <th>{{price}}</th>
        <th>{{quantity}}</th>
        <th>{{date}}</th>
        <th><img src="{{image}}" alt="" style="height:200px"></th>
        <th><iframe width="560" height="315" src="https://www.youtube.com/embed/1f-C_W4kj18" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" allowfullscreen></iframe></th>
    </tr>
        {{/each}}
</table>